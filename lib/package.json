{
	"name": "@poker-quiz/lib",
	"private": true,
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		"import": "./build/esm/index.js",
		"require": "./build/cjs/index.js"
	},
	"scripts": {
		"watch": "pnpm build:cjs --watch & pnpm build:esm --watch",
		"build": "pnpm build:cjs && pnpm build:esm",
		"build:cjs": "tsc --module commonjs --outDir ./build/cjs",
		"build:esm": " tsc --module es6 --outDir ./build/esm"
	},
	"devDependencies": {
		"typescript": "^5.0.0"
	}
}
